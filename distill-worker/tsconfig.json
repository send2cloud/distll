{
    "compilerOptions": {
        "target": "es2021",
        "lib": [
            "es2021"
        ],
        "alwaysStrict": true,
        "strict": true,
        "preserveConstEnums": true,
        "moduleResolution": "node",
        "sourceMap": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "types": [
            "@cloudflare/workers-types"
        ]
    },
    "include": [
        "src/**/*.ts"
    ]
}